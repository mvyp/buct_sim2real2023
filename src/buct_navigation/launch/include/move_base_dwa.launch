<launch>>
    <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen" clear_params="true">
        <param name="base_global_planner" value="global_planner/GlobalPlanner" />
        <param name="base_local_planner" value="dwa_local_planner/DWAPlannerROS" />
        
        <rosparam file="$(find buct_navigation)/param/dwa/dwa_local_planner_params.yaml" command="load" />
        <rosparam file="$(find buct_navigation)/param/dwa/global_planner_params.yaml" command="load" />
        
        <rosparam file="$(find buct_navigation)/param/dwa/costmap_common_params.yaml" command="load" ns="global_costmap" />
        <rosparam file="$(find buct_navigation)/param/dwa/costmap_common_params.yaml" command="load" ns="local_costmap" />
        <rosparam file="$(find buct_navigation)/param/dwa/local_costmap_params.yaml" command="load" />
        <rosparam file="$(find buct_navigation)/param/dwa/global_costmap_params.yaml" command="load" />

        <!--rosparam file="$(find buct_navigation)/param/dwa/move_base_params.yaml" command="load" /-->
           
            <!--remap from="cmd_vel" to="raw_cmd_vel"/-->

  </node>
  

  
</launch>

   
